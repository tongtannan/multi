{
  "name": "multi",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "vue-cli-service lint",
    "serve-first": "cross-env page=first vue-cli-service serve --mode development",
    "serve-second": "cross-env page=second vue-cli-service serve --mode development",
    "build-first": "cross-env page=first vue-cli-service build --mode production",
    "build-second": "cross-env page=second vue-cli-service build --mode production"
  },
  "dependencies": {
    "core-js": "^3.6.4",
    "element": "^0.1.4",
    "element-ui": "^2.13.1",
    "js-cookie": "^2.2.1",
    "ts-axios-ttn": "^0.1.0",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-plugin-router": "~4.3.0",
    "@vue/cli-plugin-vuex": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-component": "^1.1.1",
    "cache-loader": "^4.1.0",
    "compression-webpack-plugin": "^3.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.2.2",
    "hard-source-webpack-plugin": "^0.13.1",
    "image-webpack-loader": "^6.0.0",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "prettier": "^1.19.1",
    "speed-measure-webpack-plugin": "^1.3.3",
    "thread-loader": "^2.1.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "vue-cli-plugin-element": "~1.0.1",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-console": 0,
      "no-undef": 0,
      "comma-dangle": [
        "error",
        "never"
      ],
      "semi": [
        "error",
        "always"
      ],
      "eqeqeq": [
        "error",
        "always"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "trailingComma": "none"
        }
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
